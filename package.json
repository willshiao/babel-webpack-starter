{
  "name": "babel-webpack-starter",
  "version": "1.0.0",
  "private": true,
  "description": "A starter template for projects using Webpack and Babel.",
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin.js ./dist",
    "build": "npm run clean && node ./node_modules/webpack/bin/webpack.js",
    "build-dev": "node ./node_modules/webpack/bin/webpack.js --config ./webpack.config.dev.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base dist/ --config ./webpack.config.dev.js"
  },
  "author": "William Shiao",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "css-loader": "^0.24.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willshiao/babel-webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/willshiao/babel-webpack-starter/issues"
  },
  "homepage": "https://github.com/willshiao/babel-webpack-starter#readme"
}
